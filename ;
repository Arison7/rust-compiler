use crate::lexing::token::TokenKind;

pub enum ProductionRule {
    Token(TokenKind),
    Node(Node),
}

pub trait  Node {
    production_rules: Vec<ProductionRule>;
}

impl Node {
    pub fn get_production_rules(n: Node) -> Vec<ProductionRule> {
        n.production_rules
    }
    pub fn new(rules: Vec<ProductionRule>) -> Self {
        Self {
            production_rules: rules,
        }
    }
}
